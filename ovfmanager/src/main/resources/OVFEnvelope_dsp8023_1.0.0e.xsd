        <xs:annotation>
            <xs:documentation>Wrapper for CIM_VirtualSystemSettingData_Type
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vssd:CIM_VirtualSystemSettingData_Type">
                <xs:anyAttribute namespace="##any" processContents="lax" />
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="RASD_Type">
        <xs:complexContent>
            <xs:annotation>
                <xs:documentation>Wrapper for CIM_ResourceAllocationSettingData_Type
